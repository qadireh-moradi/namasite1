{
  "name": "tailwind-project",
  "version": "1.0.0",
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/css/input.css -o ./src/css/output.css",
    "watch:css": "npx tailwindcss -i ./src/css/input.css -o ./src/css/output.css --watch ; ",
